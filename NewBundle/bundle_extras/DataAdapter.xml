<?xml version="1.0" encoding="UTF-8" ?>
<jsonDataAdapter class="net.sf.jasperreports.data.json.JsonDataAdapterImpl">
	<name>ExpedienteDataAdapter</name>
	<dataFile xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="httpDataLocation">
		<method>GET</method>
		<url>$P{ENDPOINT_JSON}</url>
		<username/>
		<password/>
		<header>
			<name>Authorization</name>
			<value>$P{AUTHORIZATION}</value>
		</header>
	</dataFile>
	<language>json</language>
	<useConnection>true</useConnection>
	<timeZone xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:java="http://java.sun.com" xsi:type="java:java.lang.String">Europe/Paris</timeZone>
	<locale xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:java="http://java.sun.com" xsi:type="java:java.lang.String">en_US</locale>
	<selectExpression/>
</jsonDataAdapter>